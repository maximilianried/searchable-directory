ard\">\n        <div className=\"card-body\">\n          <div id=\"container\">\n            <div className=\"row\">\n              <div className=\"col\">\n                <div id=\"icon-container\">\n                  <i className=\"bi bi-moon-fill\" id=\"icon\"/>\n                </div>\n                <div id=\"settings-text\">Dark Mode</div>\n                <div id=\"switch-container\">\n                  <div className=\"form-check form-switch\">\n                    <input\n                      id=\"switch\"\n                      className=\"form-check-input\"\n                      type=\"checkbox\"\n                      checked={darkMode}\n                      onClick={() => {\n                        setDarkmode(!darkMode);\n                        window.location.reload();\n                      }}\n                    />\n                  </div>\n                </div>\n              </div>\n            </div>\n            <hr id=\"divider\"/>\n            <div className=\"row\">\n              <div className=\"col\">\n                <div id=\"icon-container\">\n                  <i className=\"bi bi-plus-square-fill\" id=\"icon\"/>\n                </div>\n                <div id=\"settings-text\">Neuer Tab</div>\n                <div id=\"switch-container\">\n                  <div className=\"form-check form-switch\">\n                    <input\n                      id=\"switch\"\n                      className=\"form-check-input\"\n                      type=\"checkbox\"\n                      checked={newTab}\n                      onClick={() => {\n                        setNewTab(!newTab);\n                      }}\n                    />\n                  </div>\n                </div>\n              </div>\n            </div>\n            <hr id=\"divider\"/>\n            <div className=\"row\">\n              <div className=\"col\">\n                <div id=\"icon-container\">\n                  <i className=\"bi bi-lightning-fill\